{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "no-shadow": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "global-require": 0,
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "arrow-parens": 0
  }
}
